{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,rs}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/target"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.48.0.wasm",
    "https://plugins.dprint.dev/json-0.12.1.wasm",
    "https://plugins.dprint.dev/markdown-0.9.2.wasm",
    "https://plugins.dprint.dev/toml-0.3.0.wasm",
    "https://plugins.dprint.dev/rustfmt-0.4.0.exe-plugin@c6bb223ef6e5e87580177f6461a0ab0554ac9ea6b54f78ea7ae8bf63b14f5bc2"
  ]
}
